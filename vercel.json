{
  "framework": "nuxt",
  "buildCommand": "yarn build",
  "outputDirectory": ".output",
  "installCommand": "yarn install",
  "devCommand": "yarn dev",
  "functions": {
    "app/server/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
